#import java.io.*;

public class Test
{
  public static void Main(String args[])
  {
      int a,b,c;
      System.out.println("Enter two numbers : ");
      Scanner scan=new Scanner(System.in);
      a=scan.nextInt();
      b=scan.nextInt();
      c=a+b;
      System.out.println("Sum is : "+c);
      c=a*b;
      System.out.println("Sum is : "+c);
   }
}
